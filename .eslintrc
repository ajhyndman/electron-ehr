
{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "arrow-body-style": ["error", "never"],
        "import/no-unresolved": ["off"],
        "indent": ["warn", 2],
        "new-cap": ["error", { "capIsNewExceptions": [
            "ACTIVATETAB",
            "CLOSEPATIENTSETTINGS",
            "CLOSETAB",
            "EDIT",
            "MACRO",
            "NEWTAB",
            "OPENPATIENTSETTINGS",
            "REMOVETAB",
            "TOGGLE",
            "UPDATEPATIENT",
            "List",
            "Map"
        ] }],
        "prefer-arrow-callback": ["off"]
    }
}
