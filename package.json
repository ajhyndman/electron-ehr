{
  "name": "fantasia",
  "version": "0.1.0",
  "description": "EHR charting tool built on Electron",
  "main": "main.js",
  "scripts": {
    "build": "webpack --config webpack/production.config.js",
    "build:development": "webpack --config webpack/partials/core.config.js --watch",
    "eslint": "eslint src/ --ext .js,.jsx",
    "flow": "flow",
    "package": "electron-packager . fantasia --platform=win32 --arch=x64 --overwrite",
    "start": "electron .",
    "storybook": "start-storybook -p 9001",
    "test": "npm run eslint && npm run flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajhyndman/fantasia.git"
  },
  "author": "Andrew Hyndman <ajhyndman@hotmail.com> (https://github.com/ajhyndman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajhyndman/electron-ehr/issues"
  },
  "homepage": "https://github.com/ajhyndman/electron-ehr#readme",
  "devDependencies": {
    "@flow-atom/electron": "^0.37.8-rev.0",
    "@kadira/storybook": "^1.28.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^1.2.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "flow-bin": "^0.30.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "draft-js": "^0.7.0",
    "electron-json-storage": "^2.0.0",
    "function-lift": "^1.0.5",
