{
  "name": "electron-ehr",
  "version": "0.1.0",
  "description": "EHR charting tool built on Electron",
  "main": "main.js",
  "scripts": {
    "bundle": "jspm bundle src/app.jsx --skip-source-maps --watch",
    "package": "electron-packager . electron-ehr --platform=win32 --arch=x64 --overwrite",
    "start": "start electron .",
    "storybook": "start-storybook -p 9001",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajhyndman/electron-ehr.git"
  },
  "author": "Andrew Hyndman <ajhyndman@hotmail.com> (https://github.com/ajhyndman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajhyndman/electron-ehr/issues"
  },
  "homepage": "https://github.com/ajhyndman/electron-ehr#readme",
  "devDependencies": {
    "@kadira/storybook": "^1.28.1",
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^1.2.0",
    "eslint": "^2.10.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-jspm": "^2.1.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-no-class": "^0.1.0",
    "eslint-plugin-react": "^5.1.1",
    "jspm": "git+ssh://git@github.com/jspm/jspm-cli.git#0.17.0-beta.14",
    "react": "file:jspm_packages\\npm\\react@15.1.0",
    "react-dom": "file:jspm_packages\\npm\\react-dom@15.1.0"
  },
  "dependencies": {
    "reset-css": "^2.0.2011012603"
  },
  "jspm": {
    "name": "electron-ehr",
    "main": "app.jsx",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "draft-js": "npm:draft-js@^0.7.0",
      "immutable": "npm:immutable@^3.8.1",
      "material-ui": "npm:material-ui@^0.15.0",
      "react": "npm:react@^15.1.0",
      "react-dom": "npm:react-dom@^15.1.0",
      "react-redux": "npm:react-redux@^4.4.5",
      "redux": "npm:redux@^3.5.2"
    },
    "devDependencies": {
      "babel-plugin-transform-react-jsx": "npm:babel-plugin-transform-react-jsx@^6.8.0",
      "net": "github:jspm/nodelibs-net@^0.2.0-alpha",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.11",
      "tty": "github:jspm/nodelibs-tty@^0.2.0-alpha"
    },
    "peerDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "domain": "github:jspm/nodelibs-domain@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "http": "github:jspm/nodelibs-http@^0.2.0-alpha",
      "https": "github:jspm/nodelibs-https@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
