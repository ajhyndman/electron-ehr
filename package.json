{
  "name": "electron-ehr",
  "version": "0.1.0",
  "description": "EHR charting tool built on Electron",
  "main": "main.js",
  "scripts": {
    "elm": "elm-make src/elm/Main.elm --output elm.js",
    "elm:watch": "chokidar \"**\\*.elm\" -c \"npm run elm\"",
    "main": "main.js",
    "package": "electron-packager . electron-ehr --platform=win32 --arch=x64 --overwrite",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "start npm run start & start npm run elm:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajhyndman/electron-ehr.git"
  },
  "author": "Andrew Hyndman <ajhyndman@hotmail.com> (https://github.com/ajhyndman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajhyndman/electron-ehr/issues"
  },
  "homepage": "https://github.com/ajhyndman/electron-ehr#readme",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "reset-css": "^2.0.2011012603",
    "selection": "file:src/js/selection"
  }
}
