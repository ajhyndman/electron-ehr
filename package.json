{
  "name": "electron-ehr",
  "version": "0.1.0",
  "description": "EHR charting tool built on Electron",
  "main": "main.js",
  "scripts": {
    "package": "electron-packager . electron-ehr --platform=win32 --arch=x64",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajhyndman/electron-ehr.git"
  },
  "author": "Andrew Hyndman <ajhyndman@hotmail.com> (https://github.com/ajhyndman)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajhyndman/electron-ehr/issues"
  },
  "homepage": "https://github.com/ajhyndman/electron-ehr#readme",
  "devDependencies": {
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^1.2.0",
    "eslint": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-no-class": "^0.1.0",
    "eslint-plugin-react": "^5.1.1",
    "jspm": "git+ssh://git@github.com/jspm/jspm-cli.git#0.17.0-beta.14"
  },
  "dependencies": {
    "reset-css": "^2.0.2011012603"
