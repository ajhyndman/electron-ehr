    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0",
    "seamless-immutable": "https://github.com/ajhyndman/seamless-immutable/tarball/preserve-immutable-js",
    "uuid-js": "^0.7.5"
